pipeline {
   agent any
    stages {
         stage('pre -build') {
            steps {
                sh 'echo Pre-build'
                sh 'python3 --version'
            }
        }
        stage('build') {
            steps {
                
                echo 'build in progress'
                sh 'python3  week6/Webhook/main.py'
            }
            post {
                success {
                     echo 'build in progress is success'
                }
            }
        }
        stage('long-build'){
            steps {
                echo 'long-build in progress'
            }
        }
        stage('end-build'){
            steps {
                echo 'end-build in progress'
            }
        }
    }
}
